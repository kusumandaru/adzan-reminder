{
    "name": "adzan-reminder",
    "displayName": "adzan-reminder",
    "description": "reminder adzan notification",
    "version": "1.0.0",
    "publisher": "kusumandaru",
    "author": {
      "name": "Angga Kusumandaru",
      "email": "angga.kusumandaru@gmail.com"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Adzan schedule configuration",
            "properties": {
                "adzan-reminder.city": {
                    "type": "string",
                    "default": "Jakarta",
                    "description": "selected city"
                },
                "adzan-reminder.country": {
                    "type": "string",
                    "default": "Indonesia",
                    "description": "selected country"
                }
            }
        },
        "commands": [
            {
                "command": "adzan.today",
                "title": "Adzan schedule for today"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "moment": "^2.24.0",
        "request": "^2.88.0",
        "request-promise": "^4.2.2",
        "event-stream": "3.3.4"
    },
    "keywords": [
      "Reminder",
      "Adzan",
      "Pray time",
      "Schedule"
  ]
}
